                    <Grid Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXFurnaceFlagSR01" Grid.Column="0" Fill="{Binding FurnaceStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="开炉状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                    <Grid Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXLiterFlagSR01" Grid.Column="0" Fill="{Binding LiterStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="升料机状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                    <Grid Grid.Row="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXAlarmFlagSR01" Grid.Column="0" Fill="{Binding AlarmStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="报警状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                </Grid>
            </Grid>
            <!--SR01界面完-->
            <!--DF21界面开始代码段-->
            <Grid Grid.Row="5" Grid.Column="9" Margin="0,35,0,0" Name="DF21Status">
                <Grid.RowDefinitions>
                    <RowDefinition Height="5*"></RowDefinition>
                    <RowDefinition Height="6*"></RowDefinition>
                </Grid.RowDefinitions>
                <Rectangle Grid.Row="1" Stroke="DarkGray" StrokeThickness="1" Margin="5" RadiusX="5" RadiusY="5"></Rectangle>
                <Image x:Name="ImageDF21" Grid.Row="0" Source="Images/大机-灰.png" MouseEnter="ImageDF21_MouseEnter">
                    <Image.ToolTip>
                        <TextBlock Text="{Binding Toolstip}"/>
                    </Image.ToolTip>
                </Image>
                <Grid Grid.Row="1" Margin="6,13">
                    <Grid.RowDefinitions>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <TextBlock Grid.Row="0" Text="DF21" FontSize="14" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="5,0,0,0"></TextBlock>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXMachineFlagDF21" Grid.Column="0" Fill="{Binding MachineStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="开机状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                    <Grid Grid.Row="2">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXFurnaceFlagDF21" Grid.Column="0" Fill="{Binding FurnaceStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="开炉状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                    <Grid Grid.Row="3">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXLiterFlagDF21" Grid.Column="0" Fill="{Binding LiterStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="升料机状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                    <Grid Grid.Row="4">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="22"></ColumnDefinition>
                            <ColumnDefinition></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Ellipse Name="buttonXAlarmFlagDF21" Grid.Column="0" Fill="{Binding AlarmStatus, Converter={StaticResource ColorToBrushConverter}}" 
                                 Height="12" Width="12" ></Ellipse>
                        <TextBlock Grid.Column="1" Text="报警状态" FontSize="11" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    </Grid>
                </Grid>
            </Grid>
            <!--DF21界面完-->
            <TextBlock Name="labelServerStatus" Text="{Binding ServerStatusString}" FontSize="15" Foreground="DarkGray"  Grid.Row="5" Grid.Column="10" Grid.ColumnSpan="2" VerticalAlignment="Bottom" TextAlignment="Right" Margin="0,0,10,10"/>
            <TextBlock Name="label184" Text="{Binding GuiLoadedTime}" FontSize="11" Foreground="DarkGray"  Grid.Row="4" Grid.Column="10" Grid.ColumnSpan="2" VerticalAlignment="Top" TextAlignment="Right" Margin="0,10,10,0"/>
            <DataGrid Name="dataGridViewAlarmMessage" Grid.Row="4" Grid.RowSpan="2" Grid.Column="12" Grid.ColumnSpan="7" BorderThickness="1" Margin="10" 
                      FontSize="14" AutoGenerateColumns="False" GridLinesVisibility="None" BorderBrush="DarkGray" IsReadOnly="True" RowHeaderWidth="0">
                <DataGrid.Background>
                    <SolidColorBrush Color="#FFF0F0F0" Opacity="0"/>
                </DataGrid.Background>
                <DataGrid.Columns>
                    <DataGridTextColumn Header="机器编号" Binding="{Binding MachineNo}" Width="100" ></DataGridTextColumn>
                    <DataGridTextColumn Header="报警信息" Binding="{Binding AlarmMessages}" Width="500" ></DataGridTextColumn>
                    <DataGridTextColumn Header="报警时间" Binding="{Binding TimeStamp,StringFormat='{}{0:yyyy-MM-dd HH:mm:ss}'}" Width="150"></DataGridTextColumn>
                </DataGrid.Columns>
                <DataGrid.ColumnHeaderStyle>
                    <Style TargetType="{x:Type DataGridColumnHeader}">
                        <Setter Property="Background" Value="White"/>
                        <Setter Property="BorderThickness" Value="1"/>
                        <Setter Property="BorderBrush" Value="DarkGray"/>
                        <Setter Property="Padding" Value="2"/>
                    </Style>
                </DataGrid.ColumnHeaderStyle>
            </DataGrid>
            <Rectangle Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="20" Height="2" Fill="DarkGray" VerticalAlignment="Bottom" Margin="20,0,20,25"></Rectangle>
            <Rectangle Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="20" Height="2" Fill="DarkGray" VerticalAlignment="Top" Margin="20,8,20,0"></Rectangle>
        </Grid>
    </Grid>
</Window>
